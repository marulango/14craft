body {
  font-family: 'Open Sans', sans-serif;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  color: var(--bodyColor);
  line-height: 1.25rem;
}
h1 {
  text-align: center;
  font-size: 1.25rem;
  line-height: 1.56rem;
  font-weight: bold;
}

h2 {
  font-size: 1rem;
  line-height: 1.25rem;
  font-weight: bold;
}